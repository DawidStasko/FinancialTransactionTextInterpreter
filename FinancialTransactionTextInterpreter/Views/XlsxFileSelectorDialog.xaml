 <UserControl x:Class="FinancialTransactionTextInterpreter.Views.XlsxFileSelectorDialog"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FinancialTransactionTextInterpreter.Views"
             xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
													xmlns:localization="clr-namespace:FinancialTransactionTextInterpreter.Localization"
													mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
					<ui:Button Icon="{ui:SymbolIcon DocumentData32}"
																ToolTip="{x:Static localization:Strings.MainWindow_SelectXlsxFileTooltip}"
																Click="OnClick"
																HorizontalAlignment="Stretch"
																VerticalAlignment="Stretch" />
</UserControl>
